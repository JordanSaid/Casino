<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.casino">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="casino"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".BlackjackMainActivity"
            android:label="Jays Blackjack">

            <intent-filter>

                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.MAIN"/>

            </intent-filter>
        </activity>

        <activity
            android:name=".BlackjackGameplayActivity"
            android:parentActivityName=".BlackjackMainActivity"
            android:label="Stick or Twist?">

        </activity>

    </application>

</manifest>
